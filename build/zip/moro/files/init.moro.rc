on boot
    start check

    
on early-init


on property:sys.boot_completed=1
    stop proca
    stop secure_storage
    start kernelinit


service check /data/.morokernel/scripts/check_kernel.sh
    user root
    seclabel u:r:init:s0
    oneshot
    disabled


service kernelinit /data/.morokernel/scripts/moro_init.sh
    user root
    seclabel u:r:init:s0
    oneshot
    disabled


